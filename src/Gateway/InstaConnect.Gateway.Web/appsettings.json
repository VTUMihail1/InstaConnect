{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TokenOptions": {
    "Issuer": "https://localhost:7161",
    "Audience": "https://localhost:7161",
    "AccessTokenSecurityKey": "",
    "AccountTokenSecurityKey": "",
    "AccessTokenLifetimeSeconds": 900,
    "AccountTokenLifetimeSeconds": 3600
  },
  "ReverseProxy": {
    "Routes": {
      "accounts-route": {
        "ClusterId": "users-cluster",
        "Match": {
          "Path": "/api/accounts/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/accounts/{**catch-all}"
        }
      },
      "users-route": {
        "ClusterId": "users-cluster",
        "Match": {
          "Path": "/api/users/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/users/{**catch-all}"
        }
      },
      "posts-route": {
        "ClusterId": "posts-cluster",
        "Match": {
          "Path": "/api/posts/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/posts/{**catch-all}"
        }
      },
      "post-likes-route": {
        "ClusterId": "posts-cluster",
        "Match": {
          "Path": "/api/post-likes/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/post-likes/{**catch-all}"
        }
      },
      "post-comments-route": {
        "ClusterId": "posts-cluster",
        "Match": {
          "Path": "/api/post-comments/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/post-comments/{**catch-all}"
        }
      },
      "posts-comment-like-route": {
        "ClusterId": "posts-cluster",
        "Match": {
          "Path": "/api/post-comment-likes/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/post-comment-likes/{**catch-all}"
        }
      },
      "messages-route": {
        "ClusterId": "messages-cluster",
        "Match": {
          "Path": "/api/messages/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/messages/{**catch-all}"
        }
      },
      "follows-route": {
        "ClusterId": "follows-cluster",
        "Match": {
          "Path": "/api/follows/{**catch-all}"
        },
        "Transform": {
          "PathPattern": "api/follows/{**catch-all}"
        }
      }
    }
  }
}
